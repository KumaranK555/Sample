select PRODDATE As Dates,STATE_LINK As State,LINKCITY1,MEDIA_TYPE, CITYSAT, PRODUCT_CONTANT_CATEGORY, SORTID, LINKCITYSORTID,
(Select SUM(CAST(TOT_PO_CLUS As Numeric)) As PO from prod_main where PRODDATE >= to_date('01-04-2016','DD-MM-YYYY') AND PRODDATE <= to_date('31-05-2016','DD-MM-YYYY') AND PUBLICATION_TYPE not IN ('DS') AND EDITION_CAT in ('M','P','I') AND EDTION_TYPE = 'Newspaper' 

GROUP BY prod_main1.PRODDATE,prod_main1.STATE_LINK,prod_main1.LINKCITY1,prod_main1.MEDIA_TYPE,CITYSAT,PRODUCT_CONTANT_CATEGORY, SORTID, LINKCITYSORTID),

(Select Sum(cast(PRNT_ORD_CLUS As Numeric)) AS PO From PROD_MAIN 
WHERE PRODDATE >= to_date('01-04-2016','DD-MM-YYYY') AND PRODDATE <= to_date('31-05-2016','DD-MM-YYYY') AND EDITION_CAT = 'M' AND EDTION_TYPE = 'Newspaper' AND
PRODUCTCATEGORY = (CASE PUBLICATION_TYPE WHEN 'DS' THEN 'MAIN' ELSE PRODUCTCATEGORY END)
GROUP BY PRODDATE,STATE_LINK,LINKCITY1,MEDIA_TYPE,CITYSAT,PRODUCT_CONTANT_CATEGORY, SORTID, LINKCITYSORTID)

from prod_main prod_main1
